<RelativeLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context="com.palvision.passportandcreditcardreader.PassportResult"
android:animateLayoutChanges="true">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="37dp"
        android:text="Pass Port Data" />

    <TextView
        android:id="@+id/tv_pr_issuingcountrycode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="35dp"
        android:layout_marginStart="35dp" />

    <TextView
        android:id="@+id/tv_pr_nationalitycountrycode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/tv_pr_issuingcountrycode"
        android:layout_alignLeft="@+id/tv_pr_issuingcountrycode"
        android:layout_alignStart="@+id/tv_pr_issuingcountrycode" />

    <TextView
        android:id="@+id/tv_pr_documentnumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="32dp"
        android:layout_below="@+id/tv_pr_nationalitycountrycode"
        android:layout_alignLeft="@+id/tv_pr_nationalitycountrycode"
        android:layout_alignStart="@+id/tv_pr_nationalitycountrycode" />

    <TextView
        android:id="@+id/tv_pr_document_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="25dp"
        android:layout_below="@+id/tv_pr_documentnumber"
        android:layout_alignLeft="@+id/tv_pr_documentnumber"
        android:layout_alignStart="@+id/tv_pr_documentnumber" />

    <TextView
        android:id="@+id/tv_pr_given_names"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/tv_pr_document_type"
        android:layout_alignStart="@+id/tv_pr_document_type" />

    <TextView
        android:id="@+id/tv_pr_SurNames"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/tv_pr_given_names"
        android:layout_alignLeft="@+id/tv_pr_given_names"
        android:layout_alignStart="@+id/tv_pr_given_names" />

    <TextView
        android:id="@+id/tv_pr_dayofbirth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="30dp"
        android:layout_below="@+id/tv_pr_SurNames"
        android:layout_alignLeft="@+id/tv_pr_SurNames"
        android:layout_alignStart="@+id/tv_pr_SurNames" />

    <TextView
        android:id="@+id/tv_pr_expiration_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/tv_pr_dayofbirth"
        android:layout_alignLeft="@+id/tv_pr_dayofbirth"
        android:layout_alignStart="@+id/tv_pr_dayofbirth" />

    <TextView
        android:id="@+id/tv_pr_sex"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_pr_expiration_date"
        android:layout_alignStart="@+id/tv_pr_expiration_date"
        android:layout_below="@+id/tv_pr_expiration_date"
        android:layout_marginTop="31dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv_pr_mrz_string"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_pr_sex"
        android:layout_alignStart="@+id/tv_pr_sex"
        android:layout_below="@+id/tv_pr_sex"
        android:layout_marginTop="20dp"
        android:text="TextView" />
</RelativeLayout>
